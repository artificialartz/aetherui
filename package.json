{
  "name": "aetherui",
  "version": "0.1.0",
  "description": "A high-end glassmorphism React component library featuring backdrop-filter optimizations and iridescent mesh gradient generators.",
  "type": "module",
  "main": "./dist/aetherui.cjs",
  "module": "./dist/aetherui.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/aetherui.js",
      "require": "./dist/aetherui.cjs",
      "types": "./dist/index.d.ts"
    },
    "./style.css": "./dist/style.css"
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "build:lib": "vite build --mode lib",
    "preview": "vite preview",
    "lint": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "type-check": "tsc --noEmit"
  },
  "keywords": [
    "react",
    "components",
    "glassmorphism",
    "ui",
    "backdrop-filter",
    "mesh-gradient",
    "gradient"
  ],
  "author": "",
  "license": "MIT",
  "peerDependencies": {
    "framer-motion": "^11.0.0",
    "react": "^18.0.0 || ^19.0.0",
    "react-dom": "^18.0.0 || ^19.0.0"
  },
  "devDependencies": {
    "@types/react": "18.3.28",
    "@types/react-dom": "18.3.7",
    "@typescript-eslint/eslint-plugin": "^8.15.0",
    "@typescript-eslint/parser": "^8.15.0",
    "@vitejs/plugin-react": "^4.3.4",
    "autoprefixer": "^10.4.20",
    "eslint": "^9.15.0",
    "eslint-plugin-react-hooks": "^5.0.0",
    "eslint-plugin-react-refresh": "^0.4.14",
    "framer-motion": "11.12.0",
    "postcss": "^8.4.49",
    "react": "18.3.1",
    "react-dom": "18.3.1",
    "tailwindcss": "^3.4.15",
    "typescript": "~5.6.2",
    "vite": "^6.0.1",
    "vite-plugin-dts": "^4.3.0"
  }
}
